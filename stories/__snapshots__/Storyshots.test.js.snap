    className="box">
    <img
      alt=""
      className="profile-img"
      src="login.png" />
    <form
      action="#"
      className="form-signin">
      <div
        className="error">
        Username o password non corretti
      </div>
      <input
        className="form-control"
        placeholder="Inserisci la tua email"
        required={true}
        type="text" />
      <input
        className="form-control"
        placeholder="Inserisci la tua password"
        required={true}
        type="password" />
      <button
        className="submit"
        type="button">
        Accedi
      </button>
      <div
        className="moreinfo">
        <label
          className="ricordami">
          <input
            type="checkbox"
            value="ricordami" />
           Ricordami
        </label>
        <a
          className="help"
          href="#">
          Bisogno di aiuto?
        </a>
      </div>
    </form>
  </div>
  <a
    className="registrati"
    href="#">
    Registrati
  </a>
</div>
`;

